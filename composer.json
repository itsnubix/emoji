{
  "name": "itsnubix/emoji",
  "description": "A PHP Library for interacting with Emojis",
  "type": "library",
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "pestphp/pest": "^1.21",
    "pestphp/pest-plugin-faker": "^1.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Emoji\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Kyle Milloy",
      "email": "k@fanda.io"
    }
  ],
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
